{
  "name": "ed25519-keygen",
  "version": "0.5.0-pre",
  "description": "Generate ed25519 keys for SSH, PGP (GPG), TOR, IPNS and SLIP-0010 hdkey",
  "main": "./lib/index.js",
  "module": "./lib/esm/index.js",
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {
    "@noble/curves": "~1.4.0",
    "@noble/hashes": "~1.4.0",
    "@scure/base": "~1.1.6",
    "micro-packed": "~0.5.3"
  },
  "devDependencies": {
    "@paulmillr/jsbt": "0.1.0",
    "micro-should": "0.4.0",
    "prettier": "3.1.1",
    "typescript": "5.3.2"
  },
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/esm/index.js",
      "default": "./lib/index.js"
    },
    "./ipns": {
      "types": "./lib/ipns.d.ts",
      "import": "./lib/esm/ipns.js",
      "default": "./lib/ipns.js"
    },
    "./hdkey": {
      "types": "./lib/hdkey.d.ts",
      "import": "./lib/esm/hdkey.js",
      "default": "./lib/hdkey.js"
    },
    "./ssh": {
      "types": "./lib/ssh.d.ts",
      "import": "./lib/esm/ssh.js",
      "default": "./lib/ssh.js"
    },
    "./pgp": {
      "types": "./lib/pgp.d.ts",
      "import": "./lib/esm/pgp.js",
      "default": "./lib/pgp.js"
    },
    "./tor": {
      "types": "./lib/tor.d.ts",
      "import": "./lib/esm/tor.js",
      "default": "./lib/tor.js"
    },
    "./utils": {
      "types": "./lib/utils.d.ts",
      "import": "./lib/esm/utils.js",
      "default": "./lib/utils.js"
    }
  },
  "scripts": {
    "build": "tsc && tsc -p tsconfig.esm.json",
    "lint": "prettier --check src",
    "format": "prettier --write 'src/**/*.{js,ts}' 'test/*.js'",
    "test": "node test/index.js"
  },
  "author": "Paul Miller (https://paulmillr.com)",
  "license": "MIT",
  "homepage": "https://github.com/paulmillr/ed25519-keygen",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paulmillr/ed25519-keygen.git"
  },
  "keywords": [
    "ed25519",
    "PGP",
    "GPG",
    "RFC 4880",
    "RFC 6637",
    "SSH",
    "TOR",
    "onion",
    "key generation",
    "ec",
    "elliptic"
  ],
  "funding": "https://paulmillr.com/funding/"
}
